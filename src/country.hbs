{{#each this}}

<h2>
    <p class="name">{{name}}</p>
</h2>
<div class="content">
    <div class="content__item">
        <p class="content__item-capital"><b>Capital: </b>{{capital}}</p>
        <p class="content__item-population"><b>Population: </b>{{population}}</p>
        <p class="content__item-languages"><b>Languages: </b></p>
        <ul class="content__item-list">
            {{#each languages}}
            <li class="content__item-language">{{this.name}}</li>
            {{/each}}
        </ul>
    </div>
    <div class="content__item">
        <img src="{{flag}}" alt="flag" width="320">
    </div>
</div>
{{/each}}